{
  "name": "rule-of-thumb",
  "version": "0.1.0",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint src --color",
    "test:coverage": "react-scripts test --coverage",
    "format:check": "prettier --check \"src/**/*.{js,jsx,json,ts,tsx,scss,md}\"",
    "format": "prettier --write \"src/**/*.{js,jsx,json,ts,tsx,scss,md}\" ",
    "requirements-check": "node check-node-version.js",
    "postinstall": "npm run requirements-check"
  },
  "dependencies": {
    "node-sass": "^4.14.1",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "semver": "^7.3.5",
    "web-vitals": "^0.2.4"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.13.0",
    "@testing-library/react": "^11.2.7",
    "@testing-library/user-event": "^12.8.3",
    "husky": "^4.3.8",
    "lint-staged": "^10.5.4",
    "prettier": "^2.3.1"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest",
      "plugin:jsx-a11y/recommended",
      "plugin:react-hooks/recommended"
    ],
    "plugins": [
      "jsx-a11y"
    ],
    "rules": {
      "no-console": [
        "error",
        {
          "allow": [
            "warn",
            "error"
          ]
        }
      ],
      "react/react-in-jsx-scope": "off",
      "react/sort-prop-types": 2
    }
  },
  "prettier": {
    "printWidth": 120,
    "trailingComma": "all",
    "tabWidth": 2,
    "semi": true,
    "singleQuote": true,
    "bracketSpacing": true,
    "useTabs": false,
    "jsxBracketSameLine": true,
    "jsxSingleQuote": true,
    "arrowParens": "avoid",
    "endOfLine": "lf"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": "eslint",
    "src/**/*.{js,jsx,json,ts,tsx,scss,md}": "prettier --write"
  },
  "engineStrict": true,
  "engines": {
    "node": ">=14.15.4"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
